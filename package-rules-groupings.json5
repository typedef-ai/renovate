{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  packageRules: [
    {
      matchFileNames: [
        "aqua.yaml",
        "registry.yaml",
        "aqua/aqua.yaml",
        "aqua/registry.yaml",
        ".aqua/aqua.yaml",
        ".aqua/registry.yaml",
      ],
      groupName: "â¬† aqua-packages",
      commitMessageTopic: "â¬† aqua-packages",
      prPriority: -1,
      automerge: true,
    },
    {
      matchManagers: ["custom.regex"],
      matchDepNames: ["aquaproj/aqua"],
      groupName: "â¬† aqua-packages",
    },
    {
      matchPackageNames: ["kubernetes-sigs/aws-iam-authenticator"],
      allowedVersions: "<=v0.6.14",
      automerge: true,
    },
    {
      matchManagers: ["dockerfile", "docker-compose"],
      groupName: "docker",
      commitMessageTopic: "â¬† docker",
      automerge: true,
    },
    {
      groupName: "â¬† dagger",
      commitMessageTopic: "â¬† dagger",
      automerge: true,
      matchUpdateTypes: ["minor", "patch"],
      matchPackageNames: ["/dagger/dagger/"],
    },
    { matchFileNames: ["dagger/**"], groupName: "â¬† dagger" },
    {
      matchManagers: ["custom.regex"],
      matchDepNames: ["dagger/dagger"],
      groupName: "â¬† dagger",
    },
    {
      matchPackageNames: ["kubernetes/kubectl", "kubernetes/kubernetes"],
      allowedVersions: "<=1.30",
    },
    {
      matchPackageNames: [
        "k8s.io/api",
        "k8s.io/apimachinery",
        "k8s.io/client-go",
      ],
      allowedVersions: "<=0.30",
    },
    {
      matchPackageNames: [
        "kubernetes/kubectl",
        "kubernetes/kubernetes",
        "k8s.io/api",
        "k8s.io/apimachinery",
        "k8s.io/client-go",
      ],
      groupName: "kubernetes",
      commitMessageTopic: "â¬† â˜¸ kubernetes {{depName}}",
    },
    {
      matchPackageNames: [
        "kata-containers/kata-containers",
        "quay.io/kata-containers/kata-deploy",
      ],
      groupName: "kata-containers",
      commitMessageTopic: "â¬† kata-containers {{depName}}",
    },
    { matchDatasources: ["golang-version"], rangeStrategy: "bump" },
    {
      matchManagers: ["gomod"],
      description: "reduce noise by grouping all gomod patch, minor, and digest changes together",
      commitMessageTopic: "â¬† gomod patching",
      groupName: "gomod-patch-and-minor",
      matchUpdateTypes: ["patch", "minor", "digest"],
    },
    {
      matchManagers: ["gomod"],
      description: "reduce noise by grouping all gomod major changes together",
      commitMessageTopic: "â¬† gomod major",
      groupName: "gomod-major",
      matchUpdateTypes: ["major"],
    },
    {
      matchManagers: ["nodenv", "npm"],
      groupName: "npm",
      commitMessageTopic: "â¬† node, npm",
      automerge: true,
    },
    {
      matchManagers: [
        "terraform",
        "terraform-version",
        "terragrunt",
        "terragrunt-version",
        "tflint-plugin",
      ],
      groupName: "terraform",
      commitMessageTopic: "â¬† terraform",
      automerge: true,
    },
    {
      matchManagers: ["github-actions"],
      groupName: "github-actions",
      commitMessageTopic: "â¬† github-actions",
      automerge: true,
      prPriority: -1,
      prBodyDefinitions: {
        Package: "[{{{depName}}}](https://github.com/{{{depName}}})",
        Change: "[`{{{displayFrom}}}` -> `{{{displayTo}}}`](https://github.com/{{{depName}}}/compare/{{{displayFrom}}}...{{{displayTo}}})",
      },
      prBodyColumns: ["Package", "Change", "Type", "Update"],
    },
    {
      matchFileNames: ["magefiles"],
      groupName: "mage-tooling",
      commitMessageTopic: "â¬† ðŸ¤– mage tooling",
      automerge: true,
      matchUpdateTypes: ["minor", "patch"],
      prPriority: -1,
    },
    {
      groupName: "â¬† go-x",
      commitMessageTopic: "â¬† golang.org/x packages",
      automerge: true,
      matchUpdateTypes: ["minor", "patch"],
      prPriority: -1,
      matchPackageNames: ["/golang.org/x/"],
    },
  ],
}
